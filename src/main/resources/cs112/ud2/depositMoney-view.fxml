<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs112.ud2.controllers.DepositMoneyController">
   <children>
       <Label layoutX="53.0" layoutY="53.0" prefHeight="59.0" prefWidth="208.0" text="Would you like to deposit money to your Checkings or Savings?" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Button fx:id="savingsDepositButton" layoutX="122.0" layoutY="174.0" mnemonicParsing="false" onAction="#onSavingsDepositButtonClicked" text="Savings" />
      <Button fx:id="checkingsDepositButton" layoutX="115.0" layoutY="137.0" mnemonicParsing="false" onAction="#onCheckingsDepositButtonClicked" text="Checkings" />
      <Button fx:id="homepageButton" layoutX="102.0" layoutY="249.0" mnemonicParsing="false" onAction="#onHomepageButtonClicked" text="Go to Homepage" />

   </children>
</AnchorPane>
