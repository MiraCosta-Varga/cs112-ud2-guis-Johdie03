<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs112.ud2.controllers.SavingsToCheckingsTransferController">
    <children>
        <Label layoutX="43.0" layoutY="117.0" text="Please enter amount you are transferring" />
        <Label layoutX="26.0" layoutY="66.0" text="Transferring from Savings to Checkings">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <TextField fx:id="transferAmountTextField" layoutX="84.0" layoutY="137.0" onAction="#onTransferAmountTyped" prefHeight="26.0" prefWidth="138.0" />
        <Label layoutX="74.0" layoutY="141.0" text="\$">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Button fx:id="continueButton" layoutX="116.0" layoutY="194.0" mnemonicParsing="false" onAction="#onContinueButtonClicked" text="Continue" />
        <Button fx:id="backButton" layoutX="127.0" layoutY="238.0" mnemonicParsing="false" onAction="#onBackButtonClicked" text="Back" />
    </children>
</AnchorPane>
