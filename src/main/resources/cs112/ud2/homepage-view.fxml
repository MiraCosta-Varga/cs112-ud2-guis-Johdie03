<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs112.ud2.controllers.HomepageController">
   <children>
      <Label layoutX="58.0" layoutY="97.0" text="What would you like to do with your Account?">
         <font>
            <Font size="20.0" />
         </font></Label>
      <Button fx:id="transferButton" layoutX="175.0" layoutY="244.0" mnemonicParsing="false" onAction="#onTransferButtonClicked" prefHeight="50.0" prefWidth="125.0" text="Transfer Money" />
      <Button fx:id="withdrawButton" layoutX="261.0" layoutY="175.0" mnemonicParsing="false" onAction="#onWithdrawButtonClicked" prefHeight="50.0" prefWidth="125.0" text="Withdraw Money" />
      <Button fx:id="depositButton" alignment="CENTER" layoutX="110.0" layoutY="175.0" mnemonicParsing="false" onAction="#onDepositButtonClicked" prefHeight="50.0" prefWidth="125.0" text="Deposit Money" />
   </children>
</AnchorPane>
